{
  "name": "@karthickk1996/gigcodesadmin",
  "version": "1.0.0-beta.1",
  "description": "Reusable SPA Admin Panel for Laravel",
  "main": "dist/gigcodes-admin.ssr.js",
  "browser": "dist/gigcodes-admin.esm.js",
  "module": "dist/gigcodes-admin.esm.js",
  "unpkg": "dist/gigcodes-admin.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@gigcodes-karthick"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karthickk1996/GigcodesAdmin.git"
  },
  "keywords": [
    "gigocodes",
    "admin",
    "panel"
  ],
  "author": "Karthick K",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karthickk1996/GigcodesAdmin/issues"
  },
  "homepage": "https://github.com/karthickk1996/GigcodesAdmin#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/babel-preset-app": "^4.5.15",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "underscore": "^1.13.2"
  },
  "engines": {
    "node": ">=12"
  }
}
